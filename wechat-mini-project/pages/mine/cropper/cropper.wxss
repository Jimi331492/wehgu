/* pages/mine/cropper/cropper.wxss */
.cropper-wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100vh;
    background-color: #000;
}

.cropper-buttons {
    width: 100vw;
    height: 8vh;
    background-color: #000;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: fixed;
    bottom: 0;
    left: 0;
    opacity: 1;
}



